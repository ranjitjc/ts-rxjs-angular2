{
  "name": "ts-rxjs-angular2",
  "version": "0.0.1",
  "description": "Sample application for typescript,rxjs and angular2",
  "main": "index.js",
  "scripts": {
    "start": "concurrent \"npm run tsc:w\" \"npm run lite\" ",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "lite": "lite-server",
    "build_server": "tsc server/stock_server.ts -m commonjs --outDir dist || true",
    "start_server": "npm run build_server && node dist/stock_server",
    "typings": "typings",
    "postinstall": "typings install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ranjitjc/ts-rxjs-angular2.git"
  },
  "keywords": [
    "typescript",
    "ES6",
    "rxJS",
    "angular2"
  ],
  "author": "Ranjit Jeyachandran",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ranjitjc/ts-rxjs-angular2/issues"
  },
  "homepage": "https://github.com/ranjitjc/ts-rxjs-angular2#readme",
  "dependencies": {
    "angular2": "^2.0.0-beta.13",
    "bootstrap": "^3.3.6",
    "cors": "^2.7.1",
    "d3": "^3.5.16",
    "es6-promise": "^3.0.2",
    "es6-shim": "^0.35.0",
    "express": "^4.13.4",
    "jquery": "^2.2.2",
    "jshint": "^2.9.1",
    "lodash": "^4.9.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "^5.0.0-beta.3",
    "socket.io": "^1.4.5",
    "systemjs": "^0.19.25",
    "zone.js": "^0.6.8"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.1.0",
    "typescript": "^1.8.9",
    "typings": "^0.7.9",
    "del": "2.2.0",
    "gulp": "3.9.0",
    "gulp-concat": "2.6.0",
    "gulp-sourcemaps": "1.6.0",
    "gulp-typescript": "2.10.0",
    "run-sequence": "1.1.5"
  }
}
